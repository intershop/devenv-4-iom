testframework:
  default-endpoint:
    version: "V1"
    endpoint:
      host: "${HostIom}"
      port: ${PORT_IOM_SERVICE}
      protocol: "http"
    username: "${TEST_USER_NAME}"
    password: "${TEST_USER_PASSWORD}"
  database:
    default:
      version: "V1"
      endpoint:
        host: "${PgHostExtern}:${PgPortExtern}"
      username: "${OMS_DB_USER}"
      password: "${OMS_DB_PASS}"
      parameters:
        dbName: "${OMS_DB_NAME}"
